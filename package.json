{
  "name": "redux-react-router-todomvc",
  "version": "1.0.0",
  "description": "An implementation of TodoMVC using React, Redux, React-Router, and more!",
  "repository": "tough-griff/redux-react-router-todomvc",
  "author": "Griffin Yourick",
  "license": "MIT",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "check": "npm run lint && npm run test",
    "clean": "rimraf assets coverage",
    "lint": "eslint --ext=.js,.jsx js test",
    "start": "babel-node server.js",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "test:watch": "NODE_ENV=test npm run test -- --watch",
    "test": "NODE_ENV=test mocha --recursive --compilers=js:babel-register --require ./test/helpers/jsdom.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "camel-case": "^1.2.2",
    "capitalize": "^1.0.0",
    "classnames": "^2.2.1",
    "constant-mirror": "^1.0.6",
    "fetch-check-http-status": "^1.0.0",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.3",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.5",
    "react-router": "1.0.3",
    "redux": "^3.0.5",
    "redux-router": "^1.0.0-beta5",
    "redux-thunk": "^1.0.2",
    "todomvc-app-css": "^2.0.3",
    "todomvc-common": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.13.0",
    "expect.js": "^0.3.1",
    "express": "^4.13.3",
    "fetch-mock": "3.1.3",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.1",
    "json-server": "^0.8.4",
    "mocha": "^2.3.4",
    "morgan": "^1.6.1",
    "node-libs-browser": "^0.5.3",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-mock-store": "^0.0.4",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
